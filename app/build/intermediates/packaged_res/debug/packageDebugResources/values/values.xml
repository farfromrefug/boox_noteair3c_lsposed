<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string-array name="xposedscope">
        <item>android</item>
        <item>com.android.systemui</item>
        <item>com.onyx</item>
        <item>com.onyx.kreader</item>
        <item>com.onyx.floatingbutton</item>
    </string-array>
    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#FF4081</color>
    <color name="colorPrimaryDark">#181C25</color>
    <string name="xposed_desc">Module with different improvements for the Boox Note Air 3c</string>
    <style name="App.Preference.SwitchPreference" parent="Preference.SwitchPreference.Material">
        <item name="android:widgetLayout">@layout/my_material_switch_layout</item>
    </style>
    <style name="AppTheme" parent="AppThemeOverlay">


        <item name="preferenceTheme">@style/ThemeOverlay.App.Preference</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionModeOverlay">true</item>
        <item name="android:windowActivityTransitions">true</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:windowAllowEnterTransitionOverlap">true</item>
        <item name="android:windowAllowReturnTransitionOverlap">true</item>
    </style>
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.MaterialComponents.ActionBar">
    </style>
    <style name="AppTheme.Toolbar" parent="Widget.MaterialComponents.Toolbar">
        <item name="titleTextAppearance">@style/TextAppearance.MaterialComponents.Body1</item>
    </style>
    <style name="AppThemeOverlay" parent="Theme.Material3.Light">
    </style>
    <style name="ThemeOverlay.App.Preference" parent="PreferenceThemeOverlay">
        <item name="switchPreferenceCompatStyle">@style/App.Preference.SwitchPreference</item>
    </style>
</resources>